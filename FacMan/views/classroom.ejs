<html lang="en">
  <% include ../views/partials/head %>
  <body>
    <header>
        <% include ../views/partials/header %>
    </header>

    <% include ../views/partials/sidebar %>
    
    <div class="main">
        <p><strong>Building ID:</strong> <%= classroom["classroom"]["BUILDING_ID"] %></p>
        <p><strong>Room Number:</strong> <%= classroom["classroom"]["ROOM_NUM"] %></p>
        <p><strong>Capacity:</strong> <%= classroom["classroom"]["CAPACITY"] %></p>
        
        <%if(Object.keys(classroom["reserve"]).length > 0){%> 
            <p><strong>Reservations for this Classroom:</strong></p><ul>
            <%for(i in classroom["reserve"]) { %>
                <li><%= i + ": " + classroom["reserve"][i] %></li>
            <% } }%>

        </ul>
        <p><strong>Student List:</strong> Will be putting student stuff here. <%= %></p>
        <ul>
        </ul>
        
    </div>

    <% include ../views/partials/footer %>
  </body>
</html>
